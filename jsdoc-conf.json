{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["class/identity.js",
          "class/controller.js",
          "class/identity.js",
          "include/functions.js"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
      "destination": "./docs/html/"
    }
}
